{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "build:serve": "npx http-server -p 8080 -c-1 dist/angular-boilerplate/browser",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "format-staged": "pretty-quick --staged",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "style:format": "prettier -w ./src --ignore-unknown",
    "postinstall": "npm run prettier -s",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.1",
    "@angular/cdk": "20.2.14",
    "@angular/common": "^21.1.1",
    "@angular/compiler": "^21.1.1",
    "@angular/core": "^21.1.1",
    "@angular/forms": "^21.1.1",
    "@angular/material": "20.2.14",
    "@angular/platform-browser": "^21.1.1",
    "@angular/platform-browser-dynamic": "^21.1.1",
    "@angular/router": "^21.1.1",
    "@angular/service-worker": "^21.1.1",
    "@ngneat/overview": "6.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngx-translate/core": "17.0.0",
    "@ngx-translate/http-loader": "17.0.0",
    "@ngxpert/hot-toast": "6.1.0",
    "bootstrap": "5.3.8",
    "rxjs": "^7.8.2",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.8.1",
    "zone.js": "0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.1",
    "@angular/language-service": "^21.1.1",
    "@angular/localize": "21.1.1",
    "@colsen1991/ngx-translate-extract-marker": "3.0.1",
    "@eslint/js": "^9.30.0",
    "@ngx-rocket/scripts": "^5.2.3",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^25.0.10",
    "angular-eslint": "21.1.0",
    "class-transformer": "^0.5.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "pretty-quick": "4.2.2",
    "reflect-metadata": "^0.2.1",
    "stylelint": "~17.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "~17.0.0",
    "stylelint-config-standard": "~40.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
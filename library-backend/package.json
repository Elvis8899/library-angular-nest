{
  "name": "library",
  "version": "0.0.1",
  "description": "Biblioteca",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "env-cmd -f .env.test jest --config ./test/jest.config.js --watch --runInBand",
    "test:debug": "env-cmd -f .env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:cov": "env-cmd -f .env.test jest --config ./test/jest-cov.config.js --coverage --runInBand",
    "test:e2e": "env-cmd -f .env.test jest --config ./test/jest-e2e.config.js --runInBand",
    "test:inte": "env-cmd -f .env.test jest --config ./test/jest-integration.config.js --runInBand",
    "test:unit": "env-cmd -f .env.test jest --config ./test/jest-unit.config.js",
    "test:docker:up": "docker compose -f .docker/docker-compose-test.yml up -d && wait-on tcp:5431 && env-cmd -f .env.test yarn migrate:init",
    "test:docker:down": "docker compose -f .docker/docker-compose-test.yml down",
    "migrate": "prisma migrate dev",
    "migrate:init": "prisma migrate dev --name init",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "postMigration": "ts-node -r tsconfig-paths/register --transpile-only prisma/postMigration.ts",
    "docker:dev": "docker compose --file .docker/docker-compose-dev.yml up --build -d"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.650.0",
    "@aws-sdk/client-s3": "^3.650.0",
    "@aws-sdk/s3-request-presigner": "^3.650.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/request-context": "^5.1.0",
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "^5.20.0",
    "bcrypt": "^6.0.0",
    "env-var": "^7.5.0",
    "exceljs": "^4.4.0",
    "fastify-file-interceptor": "^1.0.9",
    "fastify-multer": "^2.0.3",
    "fp-ts": "^2.16.6",
    "geodesy": "^1.1.3",
    "monocle-ts": "^2.3.13",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^4.1.0",
    "newtype-ts": "^0.3.5",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.2.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.5.6",
    "tslib": "^2.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/bcrypt": "^6.0.0",
    "@types/geodesy": "^1.2.2",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.5.3",
    "wait-on": "^8.0.1",
    "webpack": "^5.93.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --transpile-only prisma/seed.ts"
  }
}

{
  "name": "library",
  "version": "0.0.1",
  "description": "Biblioteca",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test:watch": "env-cmd -f .env.test jest --config ./test/jest.config.ts --watch --runInBand",
    "test:debug": "env-cmd -f .env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:cov": "env-cmd -f .env.test jest --config ./test/jest-cov.config.ts --coverage --runInBand",
    "test:e2e": "env-cmd -f .env.test jest --config ./test/jest-e2e.config.ts --runInBand",
    "test:inte": "env-cmd -f .env.test jest --config ./test/jest-integration.config.ts --runInBand",
    "test:unit": "env-cmd -f .env.test jest --config ./test/jest-unit.config.ts",
    "test:docker:up": "docker compose -f .docker/docker-compose-dev.yml up postgres -d && sleep 5 && env-cmd -f .env.test yarn migrate:reset",
    "test:migrate": "env-cmd -f .env.test yarn migrate:deploy",
    "migrate": "prisma migrate dev",
    "migrate:init": "prisma migrate dev --name init",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "postMigration": "ts-node -r tsconfig-paths/register --transpile-only prisma/postMigration.ts",
    "docker:dev": "docker compose --file .docker/docker-compose-dev.yml up --build -d"
  },
  "dependencies": {
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.4.0",
    "@fastify/request-context": "^6.2.1",
    "@fastify/static": "^9.0.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-fastify": "^11.1.12",
    "@nestjs/swagger": "^11.2.5",
    "@prisma/client": "^5.20.0",
    "bcrypt": "^6.0.0",
    "env-var": "^7.5.0",
    "fastify-file-interceptor": "^1.0.9",
    "fp-ts": "^2.16.6",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^4.5.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.5.6",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint/js": "^9.30.0",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.54.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --transpile-only prisma/seed.ts"
  }
}
